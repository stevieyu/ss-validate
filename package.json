{
  "name": "ss-validate",
  "description": "数据验证辅助",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stevieyu/ss-validate.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "tsup src/index.ts --watch --dts --format esm,cjs,iife",
    "build": "tsup src/index.ts --minify --dts --format esm,cjs,iife",
    "test": "tsdx test",
    "lint": "tsdx lint --fix"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
