    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
  },
  "devDependencies": {
    "lodash": "^4.17.15",
    "@types/lodash": "^4.14.168",
    "husky": "^4.2.5",
    "pretty-quick": "^2.0.1",
    "tsup": "^4.0.0",
    "typescript": "^4.1.5",
    "tslib": "^2.1.0"
  }
}
